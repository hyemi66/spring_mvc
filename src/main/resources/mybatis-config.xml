<?xml version="1.0" encoding="UTF-8"?>

<!-- 이 파일의 역할은 데이터 저장빈 클래스의 객체 별칭이름을 관리한다 -->
<!DOCTYPE configuration
PUBLIC "-//mybatis.org//DTD config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<typeAliases>
		<typeAlias type="com.naver.vo.MemberVO" alias="mem" />
		<!-- MemberVO빈클래스의 객체 별칭명 mem으로 지정 -->
		<typeAlias type="com.naver.vo.BoardVO" alias="board" />
		<typeAlias type="com.naver.vo.GuestVO" alias="guest" />
		<typeAlias type="com.naver.vo.ReplyVO" alias="reply" />
		<typeAlias type="com.naver.vo.MessageVO" alias="message" />
		<typeAlias type="com.naver.vo.UserVO" alias="user" />
		<typeAlias type="com.naver.vo.GongjiVO" alias="gongji" />
	</typeAliases>
</configuration>